<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import de.jensd.fx.glyphs.materialicons.MaterialIconView?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="462.0" prefWidth="758.0"
            xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.UsersManagerController">
    <center>
        <TableView fx:id="userTableView" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                   prefHeight="400.0" prefWidth="831.0" BorderPane.alignment="CENTER">
            <columns>
                <TableColumn fx:id="idColumn" prefWidth="45.0" text="ID"/>
                <TableColumn fx:id="nameColumn" minWidth="70.0" prefWidth="-1.0"
                             text="Full name"/>
                <TableColumn fx:id="phoneColumn" prefWidth="118.0" text="Phone"/>
                <TableColumn fx:id="addressColumn" minWidth="80.0" prefWidth="97.0"
                             text="Address"/>
                <TableColumn fx:id="nationalIdColumn" minWidth="35.0" prefWidth="90.0"
                             text="National ID"/>
                <TableColumn fx:id="genderColumn" minWidth="0.0" prefWidth="42.0"
                             text="Gender"/>
                <TableColumn fx:id="birthdayColumn" minWidth="8.0" prefWidth="93.0"
                             text="Birthday"/>
                <TableColumn fx:id="regionColumn" minWidth="0.0" prefWidth="66.0"
                             text="Region"/>
                <TableColumn fx:id="dateRegisterColumn" minWidth="2.0" prefWidth="112.0"
                             text="Date Register"/>
            </columns>
            <columnResizePolicy>
                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TableView>
    </center>
    <top>
        <GridPane BorderPane.alignment="CENTER">
            <columnConstraints>
                <ColumnConstraints hgrow="ALWAYS" maxWidth="1.7976931348623157E308" percentWidth="80.0"/>
                <ColumnConstraints hgrow="ALWAYS" percentWidth="20.0"/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES"/>
            </rowConstraints>

            <Label alignment="CENTER" contentDisplay="CENTER" maxHeight="1.7976931348623157E308"
                   maxWidth="1.7976931348623157E308" prefHeight="30.0" prefWidth="437.0" text="User Management"
                   textAlignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.vgrow="ALWAYS">
                <font>
                    <Font name="System Bold" size="25.0"/>
                </font>
            </Label>
            <JFXButton alignment="CENTER" contentDisplay="CENTER" defaultButton="true" GridPane.columnIndex="1"
                       GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER"
                       GridPane.vgrow="ALWAYS">
                <graphic>
                    <MaterialIconView glyphName="PERSON_ADD">
                        <font>
                            <Font size="38.0"/>
                        </font>
                    </MaterialIconView>
                </graphic>
                <font>
                    <Font size="28.0"/>
                </font>
            </JFXButton>
            <padding>
                <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
            </padding>
        </GridPane>
    </top>

</BorderPane>
