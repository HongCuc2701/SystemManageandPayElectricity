<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="416.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controllers.client.AddCreditCardController">
    <Label layoutX="49.0" layoutY="26.0" text="Add Credit Card">
        <font>
            <Font size="24.0"/>
        </font>
    </Label>
    <TextField layoutX="49.0" layoutY="77.0" prefHeight="49.0" prefWidth="504.0" promptText="CardHolderName"/>
    <TextField layoutX="48.0" layoutY="141.0" prefHeight="49.0" prefWidth="504.0" promptText="CardNumber"/>
    <TextField layoutX="49.0" layoutY="206.0" prefHeight="49.0" prefWidth="504.0" promptText="AccountNumber"/>
    <ComboBox layoutX="49.0" layoutY="271.0" prefHeight="49.0" prefWidth="504.0" promptText="Bank"/>
    <Button layoutX="468.0" layoutY="342.0" mnemonicParsing="false" prefHeight="49.0" prefWidth="85.0"
            style="-fx-background-color: #ec5252;" text="Save" textFill="WHITE">
        <font>
            <Font size="24.0"/>
        </font>
    </Button>
    <Button fx:id="btnCreditCardBack" layoutX="363.0" layoutY="342.0" mnemonicParsing="false"
            onAction="#backButtonOnAction" prefHeight="49.0" prefWidth="85.0" style="-fx-background-color: #ffffff;"
            text="Back">
        <font>
            <Font size="24.0"/>
        </font>
    </Button>
</AnchorPane>
